<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@*android:id/background">
        <shape>
            <solid android:color="#bb000000" />
        </shape>
    </item>
    <item android:id="@*android:id/secondaryProgress">
        <clip>
            <shape>
                <gradient android:startColor="#ff223344" android:endColor="#ffaa2244" android:angle="270.0" android:centerY="0.75" android:centerColor="#ff223344" />
            </shape>
        </clip>
    </item>
    <item android:id="@*android:id/progress">
        <clip>
            <shape>
                <solid android:color="#aa608504" />
            </shape>
        </clip>
    </item>
</layer-list>